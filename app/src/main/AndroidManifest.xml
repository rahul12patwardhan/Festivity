<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.festivity">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AlertPage"></activity>
        <activity android:name=".EventsPage2" />
        <activity android:name=".EventsPage" />
        <activity android:name=".TasksPage" />
        <activity android:name=".GroupsPage" />
        <activity android:name=".ChangeFest" />
        <activity
            android:name=".VisitorDashboard"
            android:label="@string/title_activity_visitor_dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainDashboard"
            android:label="@string/title_activity_main_dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Registration" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>